{
  "name": "cristhianbenitez.xyz",
  "description": "this is my website",
  "version": "1.0.0",
  "private": false,
  "author": "Cristhian Benitez",
  "license": "MIT",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore ./\"***/**/*.+(ts|js|tsx)\" ",
    "format": "prettier --ignore-path .gitignore \"***/**/*.+(ts|js|tsx)\" --write",
    "postinstall": "husky install",
    "commit": "cz",
    "commit-all": "git add . && cz"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "npm run lint --fix",
      "npm run format"
    ]
  },
  "dependencies": {
    "@react-spring/parallax": "9.4.4",
    "@react-spring/web": "9.4.4",
    "airtable": "0.11.3",
    "date-fns": "2.28.0",
    "framer-motion": "6.2.9",
    "gray-matter": "4.0.3",
    "next": "12.1.4",
    "next-mdx-remote": "4.0.2",
    "react": "17.0.2",
    "react-accessible-accordion": "4.0.0",
    "react-dom": "17.0.2",
    "remark": "14.0.2",
    "remark-html": "15.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "16.2.3",
    "@commitlint/config-conventional": "16.2.1",
    "@types/node": "16.11.26",
    "@types/react": "17.0.44",
    "@types/react-dom": "18.0.0",
    "@typescript-eslint/eslint-plugin": "5.18.0",
    "@typescript-eslint/parser": "5.18.0",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.12.0",
    "eslint-config-next": "12.1.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "2.7.1",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "7.0.4",
    "lint-staged": "12.3.7",
    "nanoid": "3.3.2",
    "prettier": "2.6.2",
    "typescript": "4.6.3"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
